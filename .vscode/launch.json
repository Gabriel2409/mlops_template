    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: Kedro Run",
            "cwd": "${workspaceFolder}",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "module": "kedro",
            "args": [
                "run"
            ]
        },
        {
            "name": "Python: Kedro Azure ML Run",
            "cwd": "${workspaceFolder}",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "module": "kedro",
            "justMyCode": false,
            "args": [
                "azureml",
                "run"
            ]
        }
    ]
}